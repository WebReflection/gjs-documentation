  <synopsis><code>
const GLib = imports.gi.GLib;

let pollFD = new GLib.PollFD({
    <link xref='GLib.PollFD-fd'>fd</link>: value
    <link xref='GLib.PollFD-events'>events</link>: value
    <link xref='GLib.PollFD-revents'>revents</link>: value
});
