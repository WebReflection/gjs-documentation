      <api:name>g_key_file_get_double</api:name>
      <api:arg>
        <api:type>String</api:type>
        <api:name>group_name</api:name>
      </api:arg>
      <api:arg>
        <api:type>String</api:type>
        <api:name>key</api:name>
      </api:arg>
