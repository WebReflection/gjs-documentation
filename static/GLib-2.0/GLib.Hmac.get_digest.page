  <info>
    <link xref="GLib.Hmac" group="method" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>void</api:type>
      </api:returns>
      <api:name>g_hmac_get_digest</api:name>
      <api:arg>
        <api:type>Number(guint8)</api:type>
        <api:name>buffer</api:name>
      </api:arg>
      <api:arg>
        <api:type>Number(gsize)</api:type>
        <api:name>digest_len</api:name>
      </api:arg>
    </api:function>
