    <api:function>
      <api:returns>
        <api:type>Boolean</api:type>
      </api:returns>
      <api:name>g_file_has_uri_scheme</api:name>
      <api:arg>
        <api:type>String</api:type>
        <api:name>uri_scheme</api:name>
      </api:arg>
    </api:function>
