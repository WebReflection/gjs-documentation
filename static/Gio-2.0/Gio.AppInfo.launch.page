  <info>
    <link xref="Gio.AppInfo" group="method" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>Boolean</api:type>
      </api:returns>
      <api:name>g_app_info_launch</api:name>
      <api:arg>
        <api:type>Array(Gio.File)</api:type>
        <api:name>files</api:name>
      </api:arg>
      <api:arg>
        <api:type>Gio.AppLaunchContext</api:type>
        <api:name>launch_context</api:name>
      </api:arg>
    </api:function>
