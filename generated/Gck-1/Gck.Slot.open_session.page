    <link xref="Gck.Slot" group="method" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>Gck.Session</api:type>
      </api:returns>
      <api:name>gck_slot_open_session</api:name>
      <api:arg>
        <api:type>Gck.SessionOptions</api:type>
        <api:name>options</api:name>
      </api:arg>
      <api:arg>
        <api:type>Gio.Cancellable</api:type>
