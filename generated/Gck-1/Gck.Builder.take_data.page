      <api:name>gck_builder_take_data</api:name>
      <api:arg>
        <api:type>Number(gulong)</api:type>
        <api:name>attr_type</api:name>
      </api:arg>
      <api:arg>
        <api:type>Number(guint8)</api:type>
        <api:name>value</api:name>
