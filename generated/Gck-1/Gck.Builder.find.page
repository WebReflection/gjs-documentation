      <api:returns>
        <api:type>Gck.Attribute</api:type>
      </api:returns>
      <api:name>gck_builder_find</api:name>
      <api:arg>
        <api:type>Number(gulong)</api:type>
        <api:name>attr_type</api:name>
      </api:arg>
    </api:function>
