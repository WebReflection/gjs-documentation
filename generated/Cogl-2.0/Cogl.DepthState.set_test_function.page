    <link xref="Cogl.DepthState" group="method" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>void</api:type>
      </api:returns>
      <api:name>cogl_depth_state_set_test_function</api:name>
      <api:arg>
        <api:type>Cogl.DepthTestFunction</api:type>
        <api:name>function</api:name>
      </api:arg>
    </api:function>
