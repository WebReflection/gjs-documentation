    <link xref="WebKit2.WebView" group="method" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>void</api:type>
      </api:returns>
      <api:name>webkit_web_view_save_to_file</api:name>
      <api:arg>
        <api:type>Gio.File</api:type>
        <api:name>file</api:name>
      </api:arg>
      <api:arg>
        <api:type>WebKit2.SaveMode</api:type>
        <api:name>save_mode</api:name>
      </api:arg>
      <api:arg>
        <api:type>Gio.Cancellable</api:type>
        <api:name>cancellable</api:name>
