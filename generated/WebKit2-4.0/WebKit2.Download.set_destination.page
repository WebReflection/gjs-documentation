    <api:function>
      <api:returns>
        <api:type>void</api:type>
      </api:returns>
      <api:name>webkit_download_set_destination</api:name>
      <api:arg>
        <api:type>String</api:type>
        <api:name>uri</api:name>
      </api:arg>
