    <link xref="WebKit2.PrintOperation" group="method" type="guide"/>
    <api:function>
      <api:returns>
        <api:type>WebKit2.PrintOperationResponse</api:type>
      </api:returns>
      <api:name>webkit_print_operation_run_dialog</api:name>
