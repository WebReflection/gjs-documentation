function set_attributes(schema: <link xref="Secret.Schema">Secret.Schema</link>, attributes: {String: String}, cancellable: <link href="../Gio-2.0/Gio.Cancellable.html">Gio.Cancellable</link>, callback: <link href="../Gio-2.0/Gio.AsyncReadyCallback.html">Gio.AsyncReadyCallback</link>): void {
