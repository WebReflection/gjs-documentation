function lookup_certificates_issued_by(issuer_raw_dn: Array(Number(guint8)), interaction: <link xref="Gio.TlsInteraction">Gio.TlsInteraction</link>, flags: <link xref="Gio.TlsDatabaseLookupFlags">Gio.TlsDatabaseLookupFlags</link>, cancellable: <link xref="Gio.Cancellable">Gio.Cancellable</link>): Array(<link xref="Gio.TlsCertificate">Gio.TlsCertificate</link>) {
